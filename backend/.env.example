# Backend Environment Variables
PORT=3001
NODE_ENV=development

# JWT Secret
JWT_SECRET=unity-ai-secret-key-change-in-production

# SSO Configuration (Enverus SSO)
SSO_ENTRY_POINT=https://sso.enverus.com/auth
SSO_ISSUER=unity-ai
SSO_CALLBACK_URL=http://localhost:3001/auth/callback
SSO_CERT=your-certificate-here

# Frontend URL
FRONTEND_URL=http://localhost:3000

# Session Secret
SESSION_SECRET=unity-ai-session-secret-change-in-production
